<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/filtercatagoryclick"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="right"
    android:layout_marginLeft="@dimen/text_size_eng"
    android:layout_marginTop="10dp"
    android:layout_marginRight="@dimen/text_size_eng"
    android:gravity="right">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="right"
        android:gravity="right"
        android:orientation="horizontal">


        <com.eoutlet.Eoutlet.customview.HelveticaNueAlticArabicTextView
            android:id="@+id/filtercatagorydetail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="@dimen/_10sdp"
            android:ellipsize="end"
            android:gravity="right"
            android:maxLines="2"
            android:text="@string/app_name"
            android:textColor="@color/colour_black"
            android:textSize="@dimen/_10sdp" />

        <ImageView
            android:id="@+id/checkselctor"
            android:layout_width="@dimen/text_size_eng"
            android:layout_height="@dimen/text_size_eng"
            android:layout_gravity="center_vertical"
            android:src="@drawable/ic_unchecked_full" />
    </LinearLayout>


</LinearLayout>