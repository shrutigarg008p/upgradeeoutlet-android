<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:id = "@+id/filtercatagoryclick"
    android:layout_marginRight="@dimen/text_size_eng"
    android:layout_marginLeft="@dimen/text_size_eng">
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">


        <ImageView
            android:layout_width="@dimen/text_size_eng"
            android:layout_height="@dimen/text_size_eng"
            android:id="@+id/checkselctor"

            android:layout_gravity="center_vertical"
            android:src="@drawable/ic_unchecked_full"/>

        <com.eoutlet.Eoutlet.customview.MulishRegularTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/app_name"
            android:layout_marginLeft="@dimen/_10sdp"
            android:id="@+id/filtercatagorydetail"
            android:ellipsize="end"
            android:maxLines="2"
            android:textSize="@dimen/_10sdp"
            android:textColor="@color/colour_black"/>
    </LinearLayout>



</LinearLayout>